# {{ ansible_managed }}

concurrent = {{ gitlab_runner_concurrent }}
check_interval = 0

[session_server]
  session_timeout = 1800

[[runners]]
  builds_dir = "{{ gitlab_runner_runners_builds_dir }}"
  cache_dir = "{{ gitlab_runner_runners_cache_dir }}"
  executor = "docker"
  name = "{{ gitlab_runner_runners_name }}"
  token = "{{ gitlab_runner_runners_token }}"
  url = "{{ gitlab_runner_runners_url }}"
  [runners.docker]
    cpus = "{{ gitlab_runner_runners_docker_cpus }}"
    disable_cache = false
    disable_entrypoint_overwrite = false
    image = "{{ gitlab_runner_runners_docker_image }}"
    memory = "{{ gitlab_runner_runners_docker_memory }}"
    memory_swap = "{{ gitlab_runner_runners_docker_memory_swap }}"
    oom_kill_disable = false
    privileged = false
    shm_size = 0
    tls_verify = false
    volumes = [
      "{{ gitlab_runner_runners_cache_dir }}:{{ gitlab_runner_runners_cache_dir }}",
      "/var/run/docker.sock:/var/run/docker.sock",
    ]
